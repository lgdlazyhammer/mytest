/*istanbul ignore next*/'use strict';jest.unmock('../sum'); // unmock to use the actual implementation of sum

describe('sum', function () {
  it('adds 1 + 2 to equal 3', function () {
    var sum = require('../sum');
    expect(sum(1, 2)).toBe(3);});


  it('adds 1 + 2 to equal 3', function () {
    var sum = require('../sum');
    expect(sum(2, 2)).toBe(4);});});